@model List<Storeprocedur.Models.Student>
<h1>Õpilased</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nimi</th>
            <th>Rühm</th>
            <th>Keskmine hinne</th>
            <th>Sünniaasta</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.StudentID</td>
                <td>@student.Name</td>
                <td>@student.Group</td>
                <td>@student.AverageGrade</td>
                <td>@student.BirthYear</td>
                <td>@student.Email</td>
            </tr>
        }
    </tbody>
</table>